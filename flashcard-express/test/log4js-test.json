{
  "appenders": [
    { "type": "console" },
    {
      "type": "file",
      "filename": "logfile.log",
      "maxLogSize": 20480,
      "backups": 3,
      "category": ["Server","App","Controller"]
    }
  ],
  "levels": {
    "[all]": "ERROR",
    "Server": "INFO",
    "App": "INFO",
    "Controller": "DEBUG"
  },
  "replaceConsole": true
}